package edunova;

public class E04_ForPetlja {
	
	public static void main(String[] args) {
		
		System.out.print("Osijek,");
		System.out.print("Osijek,");
		System.out.print("Osijek,");
		System.out.print("Osijek,");
		System.out.print("Osijek,");
		System.out.print("Osijek,");
		System.out.print("Osijek,");
		
		System.out.println();
		
		for(int i=0;i<10;i=i+1) {
			System.out.print("Osijek,");
		}
		
		for (int i=0;i<10;i=i+1) {
			System.out.println(i);
		}
		
	}

}
